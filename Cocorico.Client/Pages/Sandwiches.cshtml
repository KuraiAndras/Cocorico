@page "/sandwiches"
@inherits SandwichesModel

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
            @foreach (var sandwich in sandwiches)
            {
                <ul>
                    <li>@sandwich.Name</li>
                    <li>@sandwich.Id</li>
                </ul>
            }
        </div>
    </div>
</div>
